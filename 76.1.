#include<cstdio>
#include<string>
#include<fstream>
#include<iostream>
#include<cassert>

using namespace std;

int n, P[50];

string szyfr(string A)
{
    for (int i=0; i<A.size(); i++) swap(A[i], A[P[i%n]-1]);
    return A;
}

int main()
{
	ifstream in("szyfr1");
	n = 50;
	string tekst[6];
    for (int i=0; i<6; i++) in >> tekst[i];
    for (int i=0; i<n; i++) in >> P[i];
    for (int i=0; i<6; i++) cout << szyfr( tekst[i] ) << endl;
}
