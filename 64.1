#include <fstream>
#include <cstdlib>
#include <iostream>
#include <string>

using namespace std;

class file{

    ifstream inFile;
    
    public:
        file();
        ~file();
        void loop();
        bool even(string);
        bool twoCycle(string);
};


file::file(){
    inFile.open("dane_obrazki.txt");
    if (!inFile.good()){
        cerr << "pliku nie da sie otworzyc!\n";
        exit(1);
    }
}

file::~file(){
    inFile.close();
}

bool file::even(string s){
    return s.size() % 2 == 0; 
}

bool file::twoCycle(string wiersz){
	int czarne=0;
	for(int i=0;i<20;i++){
 		if (wiersz[i]=='1') 
       		czarne++; 
	}        
}

void file::loop(){
    string data;
    while (!inFile.eof()){
        inFile >> data;
        if( twoCycle(data) ) 
        cout << data << endl;
    }
}

int main(){	
	file f;
	f.loop();
	return 0;
}
