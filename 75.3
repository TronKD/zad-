#include <iostream>
#include <fstream>
#include <string>

using namespace std;

string szyfruj(string jawny, int x, int y)
{
    string wynik = jawny;
    for(int i=0; i<wynik.length(); i++)
    {
        int c = wynik[i] - 'a';
        c = c*x + y;
        c %= 26;
        wynik[i] = c + 'a';
        
    }
    return wynik;
}

int main()
{
    ifstream in("probka.txt");
    for(int i=1; i<=5; i++)
    {
        string jawne,zakodowane;
        in >> jawne;
        in >> zakodowane;
        for(int A=0; A<=25; A++)
            for(int B=0; B<=25; B++)
                if (szyfruj(jawne,A,B)==zakodowane)
                    cout << "Klucz zakodowany " << i << ": (" << A << "," << B << ")" << endl;
        for(int A=0; A<=25; A++)
            for(int B=0; B<=25; B++)
                if (szyfruj(zakodowane,A,B)==jawne)
                    cout << "Klucz jawne " << i << ": (" << A << "," << B << ")" << endl;
    }
}
