#include <iostream>
#include <fstream>
#include <string>
#include <vector>

using namespace std;

class file{
    
    ifstream plik;
    int size=0;
    ifstream
    vector <int> factors(int number);
    vector <int> czynniki;
    
    public:  
    void run();
    
};
void file::run(){
    plik.open("ciagi.txt");
    string liczba;
    vector <int> factors(int number){
    	vector <int> czynniki;
		for (int i=2; i<number/2; i++){
   			while (liczba %i ==0){
        		number /=i;
        		czynniki.push_back(i);
    		}
 		}
 	return czynniki;
	}
}

int main() {
    vector <int> czynniki(16) = factors(16);
    
    for( size_t i = 0; i < czynniki.size(); i++ )
         printf( "%d, ", czynniki[ i ] );
   
    printf( "\n\nKoniec\n" );
    
    return 0;
}
